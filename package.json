{
  "name": "vayu-ts",
  "version": "0.2.0",
  "description": "Simple and easy to use typescript package for utilizing vayu billing system",
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "dist/index.d.ts": [
        "dist/index.d.ts"
      ],
      "v2": [
        "dist/v2.d.ts"
      ]
    }
  },
  "exports": {
    ".": "./dist/index.js",
    "./v2": "./dist/v2.js"
  },
  "homepage": "https://withvayu.com",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "generate": "./generate.sh && ./generate-v2.sh",
    "build": "tsc",
    "lint": "eslint sdk",
    "lint:fix": "eslint sdk --fix"
  },
  "keywords": [
    "vayu",
    "billing",
    "events",
    "typescript",
    "sdk"
  ],
  "author": {
    "email": "team@withvayu.com",
    "name": "Vayu, Inc.",
    "url": "https://withvayu.com"
  },
  "license": "ISC",
  "dependencies": {
    "@types/node": "*",
    "@types/node-fetch": "^2.5.7",
    "es6-promise": "^4.2.4",
    "form-data": "^2.5.0",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^2.6.0",
    "url-parse": "^1.4.3"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.7",
    "@types/url-parse": "1.4.4",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "5.58.0",
    "eslint": "8.43.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-unused-imports": "2.0.0",
    "typescript": "^4.0"
  }
}
